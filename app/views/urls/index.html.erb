<h3>Enter a Url</h3>
<form action='/urls/parse_url' method="post">
	<input name="url" />
	<button>submit</button>

</form>

<ul>
	<% @urls.each do |url| %> 
	<li><%= url.url %></li>
	<% end %>
</ul>

<div class="resultbody">
	<% @results.each do |result| %> 
	<div class="singleresult">
		<p><%= result.result %></p>
	</div>
	<% end %>
</div>
